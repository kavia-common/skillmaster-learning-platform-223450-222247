# Backend environment variables (do not commit real secrets)
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3001
# Set to your frontend URL to tighten CORS (e.g., http://localhost:3000)
REACT_APP_FRONTEND_URL=http://localhost:3000
# Optional: publicly reachable backend base URL (if applicable)
REACT_APP_BACKEND_URL=http://localhost:3001
# Optional: websockets URL if used
REACT_APP_WS_URL=
# Feature flags can be JSON or comma-separated flags
REACT_APP_FEATURE_FLAGS={"newProgressUI": true}
